<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/setTime"
        android:layout_width="162dp"
        android:layout_height="66dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="108dp"
        android:layout_marginLeft="108dp"
        android:layout_marginRight="108dp"
        android:layout_marginStart="108dp"
        android:text="Set Time"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="87dp"
        android:layout_height="28dp"
        android:layout_marginEnd="35dp"
        android:layout_marginLeft="70dp"
        android:layout_marginRight="35dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="15dp"
        android:text="End Time"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView16"
        app:layout_constraintTop_toBottomOf="@+id/calendarSet" />

    <EditText
        android:id="@+id/endMinute2"
        android:layout_width="71dp"
        android:layout_height="53dp"
        android:layout_marginBottom="90dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:ems="10"
        android:hint="Minute"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <EditText
        android:id="@+id/startMinute2"
        android:layout_width="71dp"
        android:layout_height="53dp"
        android:layout_marginBottom="6dp"
        android:layout_marginTop="1dp"
        android:ems="10"
        android:hint="Minute"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/setTime"
        app:layout_constraintStart_toEndOf="@+id/startHour2"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <EditText
        android:id="@+id/startHour2"
        android:layout_width="71dp"
        android:layout_height="53dp"
        android:layout_marginBottom="89dp"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:ems="10"
        android:hint="Hour:"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/startMinute2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="92dp"
        android:layout_height="29dp"
        android:layout_marginEnd="70dp"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="70dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="15dp"
        android:text="Start Time"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/textView17"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/calendarSet" />

    <CalendarView
        android:id="@+id/calendarSet"
        android:layout_width="360dp"
        android:layout_height="349dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="9dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/endHour2"
        android:layout_width="70dp"
        android:layout_height="52dp"
        android:layout_marginBottom="9dp"
        android:ems="10"
        android:hint="Hour:"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/setTime"
        app:layout_constraintEnd_toStartOf="@+id/endMinute2"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />
</android.support.constraint.ConstraintLayout>